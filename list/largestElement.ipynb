   "source": [
    "# method 1\n",
    "def largest_element(nums:list):\n",
    "    max=nums[0]\n",
    "    if not nums:\n",
    "        return None\n",
    "    for i in range(1,len(nums)):\n",
    "        if nums[i]>max:\n",
    "            max=nums[i]\n",
    "    return max\n"
   ]
  },
