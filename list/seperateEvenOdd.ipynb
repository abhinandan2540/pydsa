   "source": [
    "def seperateEvenOdd(nums:list):\n",
    "    even=[i for i in nums if i%2==0]\n",
    "    odd=[i for i in nums if i%2!=0]\n",
    "\n",
    "    return even, odd\n"
   ]
  },
