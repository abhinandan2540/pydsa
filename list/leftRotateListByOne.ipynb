   "source": [
    "def left_rotate(nums:list):\n",
    "    x=nums[0]\n",
    "    for i in range(1, len(nums)):\n",
    "        nums[i-1]=nums[i]\n",
    "    nums[len(nums)-1]=x\n",
    "    return nums\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "fb46d342",
   "metadata": {},
