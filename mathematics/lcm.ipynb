   "source": [
    "def gcd(a:int, b:int):\n",
    "    if (b==0):\n",
    "        return a\n",
    "    return gcd(b, a%b)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "b352b412",
   "metadata": {},
   "outputs": [],
   "source": [
    "def lcm(a:int, b:int):\n",
    "    return a*b//gcd(a,b)\n"
   ]
  },
