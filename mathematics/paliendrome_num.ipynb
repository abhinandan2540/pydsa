   "source": [
    "def num_paliendrome(n:int):\n",
    "    rev_num=0\n",
    "    ori_num=n\n",
    "    while(n>0):\n",
    "        ld=n%10\n",
    "        rev_num=rev_num*10+ld\n",
    "        n=n//10\n",
    "    return rev_num==ori_num\n"
   ]
