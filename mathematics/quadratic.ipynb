    "import math\n",
    "\n",
    "def quadratic_equation(a:int, b:int, c:int):\n",
    "    det=((b*b)-(4*a*c))\n",
    "    d=math.sqrt(det)\n",
    "    if(d>=0):\n",
    "        x1=math.floor(((-b)+(d))/(2*a))\n",
    "        x2=math.floor(((-b)-(d))/(2*a))\n",
    "    else:\n",
    "        return -1\n",
    "    return x1,x2\n",
    "        \n",
    "print(quadratic_equation(1,-2,1))"
   ]
  }
